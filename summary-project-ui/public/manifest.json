{
  "manifest_version": 3,
  "name": "Summarizer",
  "description": "Extension to retrieve a website's overview",
  "version": "1.0",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "action": {
    "default_popup": "index.html"
  }
}